    "type": "library",
    "keywords": ["sugarcrm"],
    "authors": [
        {
            "name": "Emmanuel Dyan",
            "email": "emmanuel.dyan@inetprocess.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3",
        "psr/log": "~1.0",
        "symfony/finder": "~2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
